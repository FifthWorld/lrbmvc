@model LRB.Lib.Domain.Property

<script src="~/Scripts/jquery-1.8.2.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Property</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.Developed)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Developed)
            @Html.ValidationMessageFor(model => model.Developed)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.CapacityofOwnership)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.CapacityofOwnership)
            @Html.ValidationMessageFor(model => model.CapacityofOwnership)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.LandUse)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.LandUse)
            @Html.ValidationMessageFor(model => model.LandUse)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.PeriodofPossession)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.PeriodofPossession)
            @Html.ValidationMessageFor(model => model.PeriodofPossession)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ApproximateArea)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ApproximateArea)
            @Html.ValidationMessageFor(model => model.ApproximateArea)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
